<Application x:Class="PracticeProject.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PracticeProject"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styleses\Colors.xaml" />
                <ResourceDictionary Source="Styleses\Fonts.xaml" />
                <ResourceDictionary Source="Styleses\Buttons.xaml" />
                <ResourceDictionary Source="Styleses\ContentControls.xaml" />
                <ResourceDictionary Source="Images.xaml"/>

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
